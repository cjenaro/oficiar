generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "rhel-openssl-1.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Trader {
  id          Int    @id @default(autoincrement())
  created_at  DateTime? @default(now()) @db.Timestamptz(6)
  name        String?
  rating      Int?
  description String?
  number      String?
  trade       String?
}
